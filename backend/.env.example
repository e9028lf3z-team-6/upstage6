# Upstage API Key (Required for analysis)
# Get your key at https://console.upstage.ai/
UPSTAGE_API_KEY=your_upstage_api_key 

# Database URL
# Default: sqlite+aiosqlite:///./data/team.db
DB_URL=sqlite+aiosqlite:///./data/team.db

# Frontend Origin (CORS)
# Default: http://localhost:5173
FRONTEND_ORIGIN=http://localhost:5173

# LangSmith (Observability / Eval)
# Enable tracing: LANGSMITH_TRACING=true
LANGSMITH_API_KEY=your_langsmith_api_key
LANGSMITH_PROJECT=team-contextor
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_TRACING=true
